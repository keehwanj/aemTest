<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <setting
                jcr:primaryType="nt:unstructured"
                jcr:title="Setting"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Header text"
                                name="./text"
                                xtype="textfield"/>
                        </items>
                    </column>
                </items>
            </setting>
            <type
                jcr:primaryType="nt:unstructured"
                jcr:title="Header Type"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <type
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Type"
                                name="./type">
                                <items jcr:primaryType="nt:unstructured">
                                    <display-1
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 display-1"
                                        value="display-1"/>
                                    <display-2
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 display-2"
                                        value="display-2"/>
                                    <display-3
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 display-3"
                                        value="display-3"/>
                                    <display-4
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 display-4"
                                        value="display-4"/>
                                    <display-5
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 display-5"
                                        value="display-5"/>
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="h1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="h2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="h3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="h4"
                                        value="h4"/>
                                    <text-crimson
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 text-crimson"
                                        value="text-crimson"/>
                                    <text-forest
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 text-forest"
                                        value="text-forest"/>
                                    <text-ocean
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 text-ocean"
                                        value="text-ocean"/>
                                    <text-gold
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 text-gold"
                                        value="text-gold"/>
                                    <p-2_x0020_wrapper-ocean
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 p-2 wrapper-ocean"
                                        value="p-2 wrapper-ocean"/>
                                    <p-2_x0020_wrapper-crimson
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 p-2 wrapper-crimson"
                                        value="p-2 wrapper-crimson"/>
                                    <p-2_x0020_wrapper-forest
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 p-2 wrapper-forest"
                                        value="p-2 wrapper-forest"/>
                                    <p-2_x0020_wrapper-gold
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 p-2 wrapper-gold"
                                        value="p-2 wrapper-gold"/>
                                    <p-2_x0020_wrapper-black
                                        jcr:primaryType="nt:unstructured"
                                        text="h1 p-2 wrapper-black"
                                        value="p-2 wrapper-black"/>
                                </items>
                            </type>
                        </items>
                    </column>
                </items>
            </type>
            <anchor
                jcr:primaryType="nt:unstructured"
                jcr:title="Anchor"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <anchor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Anchor ID"
                                name="./anchor"
                                xtype="textfield"/>
                        </items>
                    </column>
                </items>
            </anchor>
        </items>
    </content>
</jcr:root>
