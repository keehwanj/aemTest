<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Search"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <languagesText
            jcr:primaryType="cq:Widget"
            fieldLabel="Languages label"
            name="./languagesText"
            xtype="textfield"/>
        <lastModifiedText
            jcr:primaryType="cq:Widget"
            fieldLabel="Next label"
            name="./nextText"
            xtype="textfield"/>
        <nextText
            jcr:primaryType="cq:Widget"
            fieldLabel="Last Modified label"
            name="./lastModifiedText"
            xtype="textfield"/>
        <noResultsText
            jcr:primaryType="cq:Widget"
            fieldLabel="No results text"
            name="./noResultsText"
            rows="{Long}4"
            xtype="textarea"/>
        <previousText
            jcr:primaryType="cq:Widget"
            fieldLabel="Previous label"
            name="./previousText"
            xtype="textfield"/>
        <refineResultsText
            jcr:primaryType="cq:Widget"
            fieldLabel="Refine Results label"
            name="./refineResultsText"
            xtype="textfield"/>
        <relatedSearchesText
            jcr:primaryType="cq:Widget"
            fieldLabel="Related searches text"
            name="./relatedSearchesText"
            xtype="textfield"/>
        <removeFilterText
            jcr:primaryType="cq:Widget"
            fieldLabel="Remove Filter label"
            name="./removeFilterText"
            xtype="textfield"/>
        <resultPagesText
            jcr:primaryType="cq:Widget"
            fieldLabel="Result pages label"
            name="./resultPagesText"
            xtype="textfield"/>
        <searchTitle
            jcr:primaryType="cq:Widget"
            fieldLabel="Search Title label"
            name="./searchTitle"
            xtype="textfield"/>
        <searchTrendsText
            jcr:primaryType="cq:Widget"
            fieldLabel="Search trends text"
            name="./searchTrendsText"
            xtype="textfield"/>
        <similarPagesText
            jcr:primaryType="cq:Widget"
            fieldLabel="Similar pages text"
            name="./similarPagesText"
            xtype="textfield"/>
        <spellcheckText
            jcr:primaryType="cq:Widget"
            fieldLabel="Spellcheck Text"
            name="./spellcheckText"
            xtype="textfield"/>
        <statisticsText
            jcr:primaryType="cq:Widget"
            fieldLabel="Statistics text"
            name="./statisticsText"
            xtype="textfield"/>
        <searchButtonText
            jcr:primaryType="cq:Widget"
            fieldLabel="Search button text"
            name="./searchButtonText"
            xtype="textfield"/>
        <departmentId
            jcr:primaryType="cq:Widget"
            fieldLabel="Department Id"
            name="./departmentId"
            xtype="textfield"/>
        <hideCriteria
            jcr:primaryType="cq:Widget"
            fieldLabel="Hide Criteria"
            inputValue="false"
            name="./hideCriteria"
            type="checkbox"
            xtype="selection"/>
    </items>
</jcr:root>
