<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Service Search Input"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Service Search Input">
                <items jcr:primaryType="cq:WidgetCollection">
                    <path
                        jcr:primaryType="nt:unstructured"
                        fieldLabel="Path"
                        name="./searchResultsPath"
                        validateOnBlur="true"
                        xtype="pathfield"/>
                    <searchbutton
                        jcr:primaryType="nt:unstructured"
                        fieldLabel="Search Button Text"
                        name="./searchbutton"
                        validateOnBlur="true"
                        xtype="textfield"/>
                    <searchheader
                        jcr:primaryType="nt:unstructured"
                        fieldLabel="Search Header"
                        name="./searchheader"
                        validateOnBlur="true"
                        xtype="textfield"/>
                    <inputkeyword
                        jcr:primaryType="nt:unstructured"
                        fieldLabel="Input Keyword text"
                        name="./inputkeyword"
                        validateOnBlur="true"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
