                jcr:primaryType="cq:Panel"
                border="false"
                height=""
                title="Button"
                width="">
                <items jcr:primaryType="cq:WidgetCollection">
                    <submit
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Submit"
                        name="./submit"/>
                    <clear
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Clear"
                        name="./clear"/>
                </items>
            </button>
            <email
                jcr:primaryType="cq:Panel"
                border="false"
                height=""
                title="Email"
                width="">
                <items jcr:primaryType="cq:WidgetCollection">
                    <email
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Recipient email"
                        name="./email_address"/>
                </items>
            </email>
        </items>
    </items>
</jcr:root>
