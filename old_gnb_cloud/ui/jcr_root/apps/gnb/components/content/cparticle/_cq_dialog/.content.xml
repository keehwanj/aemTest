<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="LG Community Profile Article Viewer"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="en/cq/current/wcm/default_components.html#List">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <text-header
                jcr:primaryType="nt:unstructured"
                jcr:title="Texts"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <address
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Address or Cité"
                                fieldLabel="Address"
                                name="./address_Text"/>
                            <header_cities
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Cities or Cité"
                                fieldLabel="Cities"
                                name="./header_cities"/>
                            <header_contact
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Contact"
                                fieldLabel="Contact"
                                name="./header_contact"/>
                            <header_mayor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Mayor or Maire"
                                fieldLabel="Mayor"
                                name="./header_mayor"/>
                            <header_councillors
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Councillors or Conseillers / Conseillères"
                                fieldLabel="Councillors"
                                name="./header_councillors"/>
                            <telephone
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Tel:  / Tél : "
                                fieldLabel="Telephone"
                                name="./telephone_Text"/>
                            <fax
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Fax: / Téléc. : "
                                fieldLabel="Fax"
                                name="./fax_Text"/>
                            <email
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Email: / Email :  "
                                fieldLabel="Email"
                                name="./email_Text"/>
                            <nb
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : NB / (N.-B.)"
                                fieldLabel="NB text"
                                name="./nb_Text"/>
                            <working_language
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Working Language / Langue"
                                fieldLabel="Working Language"
                                name="./language_Text"/>
                            <county
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : County or Comté"
                                fieldLabel="County"
                                name="./county_Text"/>
                        </items>
                    </columns>
                </items>
            </text-header>
            <renderer_x0020_page
                jcr:primaryType="nt:unstructured"
                jcr:title="Default Image"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <default_image
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Default Image"
                                name="./default_image"/>
                        </items>
                    </columns>
                </items>
            </renderer_x0020_page>
        </items>
    </content>
</jcr:root>
