<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Art Bank Image Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <header
                jcr:primaryType="nt:unstructured"
                jcr:title="Header text"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <artist_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Artist text"
                                fieldLabel="Artist text"
                                name="./artist_key"/>
                            <title_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Title of Work text"
                                fieldLabel="Title of Work text"
                                name="./title_key"/>
                            <category_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Category text"
                                fieldLabel="Category text"
                                name="./category_key"/>
                            <yearCreated_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Year Created text"
                                fieldLabel="Year Created text"
                                name="./yearCreated_key"/>
                            <yearPurchased_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Year Purchased text"
                                fieldLabel="Year Purchased text"
                                name="./yearPurchased_key"/>
                        </items>
                    </columns>
                </items>
            </header>
            <resources
                jcr:primaryType="nt:unstructured"
                jcr:title="Resources"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <database
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Data Sources - AEMArtbankDev / AEMArtbankPro"
                                fieldLabel="Data Source"
                                name="./database"/>
                            <show_entries
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Numbers of entries showing - 10, 25, 50, or 100"
                                fieldLabel="Number of Entries"
                                name="./show_entries"/>
                            <null_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Null setting in Year Created, Year Purchased (n.d.)"
                                fieldLabel="Setting null"
                                name="./null_key"/>
                        </items>
                    </columns>
                </items>
            </resources>
            <modal
                jcr:primaryType="nt:unstructured"
                jcr:title="Modal text"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <artist_m_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Modal Artist text"
                                fieldLabel="Artist text"
                                name="./artist_m_key"/>
                            <title_m_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Modal Title of Work text"
                                fieldLabel="Title of Work text"
                                name="./title_m_key"/>
                            <category_m_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Modal Category text"
                                fieldLabel="Category text"
                                name="./category_m_key"/>
                            <yearCreated_m_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Modal Year Created text"
                                fieldLabel="Year Created text"
                                name="./yearCreated_m_key"/>
                            <yearPurchased_m_key
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Input Modal Year Purchased text"
                                fieldLabel="Year Purchased text"
                                name="./yearPurchased_m_key"/>
                        </items>
                    </columns>
                </items>
            </modal>
        </items>
    </content>
</jcr:root>
