<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Health Careers List Component"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="en/cq/current/wcm/default_components.html#List">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <list
                jcr:primaryType="nt:unstructured"
                jcr:title="List"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <listFrom
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Build list using"
                                name="./listFrom"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <children
                                        jcr:primaryType="nt:unstructured"
                                        text="Child pages"
                                        value="children"/>
                                    <querybuilder
                                        jcr:primaryType="nt:unstructured"
                                        text="Advanced Search"
                                        value="querybuilder"/>
                                    <search
                                        jcr:primaryType="nt:unstructured"
                                        text="Search"
                                        value="search"/>
                                    <static
                                        jcr:primaryType="nt:unstructured"
                                        text="Fixed list"
                                        value="static"/>
                                </items>
                            </listFrom>
                            <orderBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/autocomplete"
                                fieldDescription="Enter a property like 'jcr:created'"
                                fieldLabel="Order by"
                                name="./orderBy">
                                <options
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/autocomplete/list"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <created
                                        jcr:primaryType="nt:unstructured"
                                        value="jcr:created"/>
                                    <deadline
                                        jcr:primaryType="nt:unstructured"
                                        value="deadline"/>
                                    <modified
                                        jcr:primaryType="nt:unstructured"
                                        value="cq:lastModified"/>
                                    <template
                                        jcr:primaryType="nt:unstructured"
                                        value="cq:template"/>
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        value="jcr:title"/>
                                </items>
                            </orderBy>
                        </items>
                    </columns>
                </items>
            </list>
            <children
                jcr:primaryType="nt:unstructured"
                jcr:title="Child pages"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <parentPage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Leave empty to use current page"
                                fieldLabel="Parent page"
                                name="./parentPage"/>
                        </items>
                    </columns>
                </items>
            </children>
            <static
                jcr:primaryType="nt:unstructured"
                jcr:title="Fixed list"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <pages
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldDescription="Click the '+' to add a new page"
                                fieldLabel="Pages">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    name="./pages"/>
                            </pages>
                        </items>
                    </columns>
                </items>
            </static>
            <search
                jcr:primaryType="nt:unstructured"
                jcr:title="Search"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <query
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Search query"
                                name="./query"/>
                            <searchIn
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Leave empty to use '/content'"
                                fieldLabel="Start in"
                                name="./searchIn"/>
                        </items>
                    </columns>
                </items>
            </search>
            <querybuilder
                jcr:primaryType="nt:unstructured"
                jcr:title="Advanced Search"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <savedquery
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                fieldLabel="Querybuilder predicate notation"
                                name="./savedquery"/>
                        </items>
                    </columns>
                </items>
            </querybuilder>
            <renderer_x0020_page
                jcr:primaryType="nt:unstructured"
                jcr:title="Renderer Page"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <renderer_page
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Default : /content/gnb/fr/corporate/abc/perspectives_actuelles/abc_renderer OR /content/gnb/fr/corporate/abc/perspectives_actuelles/abc_renderer"
                                fieldLabel="Render Page"
                                name="./renderer_page"/>
                        </items>
                    </columns>
                </items>
            </renderer_x0020_page>
            <text
                jcr:primaryType="nt:unstructured"
                jcr:title="Text"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <header_career_title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Career Title or Title de la prefession"
                                name="./header_career_title"/>
                            <header_study_length
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Length of Study or DurÃ©e des etudes"
                                name="./header_study_length"/>
                            <header_salary
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : Salary or Salaire"
                                name="./header_salary"/>
                            <year
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Default : year or ans"
                                name="./year"/>
                        </items>
                    </columns>
                </items>
            </text>
        </items>
    </content>
</jcr:root>
