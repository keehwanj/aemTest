                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                name="./alternatedisplay1@Delete"
                                value="{Boolean}true"/>
                            <rendererPage1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Default : /content/gnb/en/multimedia/mmrenderer&#xa;OR &#xa;/content/gnb/fr/multimedia/mmrenderer"
                                fieldLabel="Renderer Page"
                                name="./rendererPage1"/>
                        </items>
                    </columns>
