<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="ServiceSearch (Design)"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <newsSearchResultsPath
            jcr:primaryType="cq:Widget"
            fieldDescription="The path to the news search results page"
            fieldLabel="Search Results Path"
            hideTrigger="true"
            name="./newsSearchResultsPath"
            regex="/^\\/.*/"
            regexText="Path must start with a '/'"
            xtype="pathfield"/>
        <newsTagValuesPath
            jcr:primaryType="cq:Widget"
            fieldDescription="The Tag Values of the DropDown"
            fieldLabel="Tag Values Path"
            hideTrigger="true"
            name="./newsTagValuesPath"
            regex="/^\\/.*/"
            regexText="Path must start with a '/'"
            xtype="pathcompletion"/>
    </items>
</jcr:root>
