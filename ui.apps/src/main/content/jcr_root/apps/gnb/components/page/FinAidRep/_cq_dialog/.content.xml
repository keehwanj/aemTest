<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Search"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <nextText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Last Modified label"
                        name="./lastModifiedText"/>
                    <noResultsText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                        fieldLabel="No results text"
                        name="./noResultsText"/>
                    <previousText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Previous label"
                        name="./previousText"/>
                    <relatedSearchesText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Related searches text"
                        name="./relatedSearchesText"/>
                    <resultPagesText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Result pages label"
                        name="./resultPagesText"/>
                    <searchTrendsText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Search trends text"
                        name="./searchTrendsText"/>
                    <similarPagesText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Similar pages text"
                        name="./similarPagesText"/>
                    <spellcheckText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Spellcheck Text"
                        name="./spellcheckText"/>
                    <statisticsText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Statistics text"
                        name="./statisticsText"/>
                    <removeFilterText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Remove Filter label"
                        name="./removeFilterText"/>
                    <refineResultsText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Refine Results label"
                        name="./refineResultsText"/>
                    <languagesText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Languages label"
                        name="./languagesText"/>
                    <lastModifiedText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Next label"
                        name="./nextText"/>
                    <searchTitle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Search Title label"
                        name="./searchTitle"/>
                    <serviceSearchButtonText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Search button text"
                        name="./serviceSearchButtonText"/>
                    <searchTag
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/autocomplete"
                        fieldLabel="Search Tag"
                        multiple="{Boolean}true"
                        name="./tagid">
                        <datasource
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/gui/components/common/datasources/tags"/>
                        <values
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/autocomplete/tags"/>
                        <options
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/autocomplete/list"/>
                    </searchTag>
                    <displayTag
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/autocomplete"
                        fieldLabel="Display Tag"
                        multiple="{Boolean}true"
                        name="./displayTagid">
                        <datasource
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/gui/components/common/datasources/tags"/>
                        <values
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/autocomplete/tags"/>
                        <options
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/autocomplete/list"/>
                    </displayTag>
                    <hideCriteria
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                        name="./hideCriteria"
                        text="Hide Criteria"
                        value="false"/>
                    <hidecriteria-delete
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                        name="./hideCriteria@Delete"
                        value="{Boolean}true"/>
                    <hideFacets
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                        name="./hideFacets"
                        text="Hide Facets"
                        value="false"/>
                    <hidefacets-delete
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                        name="./hideFacets@Delete"
                        value="{Boolean}true"/>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>
