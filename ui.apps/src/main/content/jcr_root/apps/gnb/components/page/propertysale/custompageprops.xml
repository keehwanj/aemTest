                    jcr:primaryType="cq:Widget"
                    fieldDescription="Click the '+' to add a new doc."
                    fieldLabel="Map (Link or pdf)"
                    name="./maps"
                    xtype="multifield">
                    <fieldConfig
                        jcr:primaryType="cq:Widget"
                        rootPath="/content/dam/gnb/Departments/trans/surplus"
                        xtype="pathfield"/>
                </maps>
