                    jcr:primaryType="cq:Widget"
                    defaultValue="1"
                    fieldLabel="Rank"
                    name="./rank"
                    type="select"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <_x0031_
                            jcr:primaryType="nt:unstructured"
                            text="1"
                            value="1"/>
                        <_x0032_
                            jcr:primaryType="nt:unstructured"
                            text="2"
                            value="2"/>
                    </options>
                </rank>
                <kind
                    jcr:primaryType="cq:Widget"
                    defaultValue="tender"
