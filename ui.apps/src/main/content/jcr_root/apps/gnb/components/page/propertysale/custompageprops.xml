                            value="Gloucester"/>
                        <kent
                            jcr:primaryType="nt:unstructured"
                            text="Kent"
                            value="Kent"/>
                        <kings
                            jcr:primaryType="nt:unstructured"
                            text="Kings"
                            value="Kings"/>
                        <madawaska
                            jcr:primaryType="nt:unstructured"
                            text="Madawaska"
                            value="Madawaska"/>
                        <northumberland
                            jcr:primaryType="nt:unstructured"
                            text="Northumberland"
                            value="Northumberland"/>
                        <queens
                            jcr:primaryType="nt:unstructured"
                            text="Queens"
                            value="Queens"/>
                        <restigouche
                            jcr:primaryType="nt:unstructured"
                            text="Restigouche"
                            value="Restigouche"/>
                        <saint_john
                            jcr:primaryType="nt:unstructured"
                            text="Saint John"
                            value="Saint John"/>
                        <sunbury
                            jcr:primaryType="nt:unstructured"
                            text="Sunbury"
                            value="Sunbury"/>
                        <victoria
                            jcr:primaryType="nt:unstructured"
                            text="Victoria"
                            value="Victoria"/>
                        <westmorland
                            jcr:primaryType="nt:unstructured"
                            text="Westmorland"
                            value="Westmorland"/>
                        <york
                            jcr:primaryType="nt:unstructured"
                            text="York"
                            value="York"/>
                    </options>
                </county>
                <estimate_value
                    jcr:primaryType="cq:Widget"
                    fieldDescription="If estimate value is 0, then this value is not displayed except page component."
