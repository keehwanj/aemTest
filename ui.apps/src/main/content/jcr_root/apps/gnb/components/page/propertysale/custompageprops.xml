                    name="./county"
                    type="select"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <albert
                            jcr:primaryType="nt:unstructured"
                            text="Albert"
                            value="Albert"/>
                        <carleton
                            jcr:primaryType="nt:unstructured"
                            text="Carleton"
                            value="Carleton"/>
