SetEnvIf %{REQUEST_URI} ^ GNB_ENGLISH_ERROR_PAGE=/en/error/404.html
SetEnvIf %{REQUEST_URI} ^ GNB_FRENCH_ERROR_PAGE=/fr/erreurs/404.html

# non-www to www redirect
RewriteCond %{HTTP_HOST} !^www\. [NC]
RewriteCond %{HTTP_HOST} ^(dev|qa|staging)\.gnb\.ca$ [NC]
RewriteRule ^ https://www-%1.gnb.ca%{REQUEST_URI} [L,NE,R=301]
RewriteCond %{HTTP_HOST} ^gnb\.ca$ [NC]
RewriteRule ^ https://www.gnb.ca%{REQUEST_URI} [L,NE,R=301]

RewriteRule ^/?$ /content/GNB3/home.html [PT,L]

#robots.txt for www.gnb.ca
RewriteRule ^/robots\.txt$ /content/GNB3/robots.txt [PT,L]
#RewriteCond %{REQUEST_URI} ^/robots\.txt$
#RewriteRule ^(.*)$ /robots.txt [PT,L]

RewriteCond %{REQUEST_URI} !^/apps
RewriteCond %{REQUEST_URI} !^/bin
RewriteCond %{REQUEST_URI} !^/content
RewriteCond %{REQUEST_URI} !^/etc
RewriteCond %{REQUEST_URI} !^/home
RewriteCond %{REQUEST_URI} !^/libs
RewriteCond %{REQUEST_URI} !^/saml_login
RewriteCond %{REQUEST_URI} !^/system
RewriteCond %{REQUEST_URI} !^/tmp
RewriteCond %{REQUEST_URI} !^/var
RewriteCond %{REQUEST_URI} (.html|.jpe?g|.png|.svg|.xml)$
#RewriteRule ^/(.*)$ /$1 [PT,L]
RewriteRule ^/(.*)$  /content/GNB3/$1 [PT,L]

# Rewrite rules - these 3 lines configuration is for setting keywords through acscommons
RewriteMap map.legacy "dbm:/etc/httpd/conf/tmp-gnb-redirects.map"
RewriteCond ${map.legacy:$1} !=""
RewriteRule ^(.*)$ ${map.legacy:$1|/} [L,R=301]

#TO TEST ON DEV
#RewriteCond %{HTTP_HOST} ^(www-)?(dev|qa|staging)\.gnb\.ca$ [NC]
#RewriteCond %{HTTP_HOST} !^www-(dev|qa|staging)\. [NC]
#RewriteRule ^(.*)$ http://www-%2.gnb.ca%{REQUEST_URI} [R=301,L]

# Mask the /content/GNB3/ path from URLs
RewriteRule ^/content/GNB3/(.*)(\.html)?$ /$1 [NE,L,R=301]


#Global redirect for www2.gnb.ca content
RewriteCond %{REQUEST_URI} ^/content/gnb/(.*)$
RewriteRule ^(.*)$ https://www2.gnb.ca$1 [R=301,L]

#Global redirect for www2.snb.ca content
#Commenting below lines as Alex confirmed cross-domain issue does not occur anymore - 13 Jan 2025
#RewriteCond %{REQUEST_URI} ^/content/snb/(.*)$
#RewriteRule ^(.*)$ https://www2.snb.ca$1 [R=301,L]
 
